<FindBugsFilter>

  <!-- ignore errors about missing serial version ids -->
  <Match>
    <Bug pattern="SE_NO_SERIALVERSIONID" />
  </Match>

  <!--  not internationalized at this point -->
  <Match>
    <Bug pattern="DM_CONVERT_CASE" />
  </Match>


  <!-- spotbugs issue with JDK 11+
https://github.com/spotbugs/spotbugs/issues/756 -->
  <Match>
    <Bug pattern="RCN_REDUNDANT_NULLCHECK_OF_NONNULL_VALUE" />
  </Match>

  <!-- spotbugs issue https://github.com/spotbugs/spotbugs/issues/811
-->
  <Match>
    <Bug pattern="UPM_UNCALLED_PRIVATE_METHOD" />
  </Match>

  <!--  TODO: ignore exposing of internal state for now, but may want to change this later -->
  <Match>
    <Bug pattern="EI_EXPOSE_REP" />
  </Match>
  <Match>
    <Bug pattern="EI_EXPOSE_REP2" />
  </Match>
  
</FindBugsFilter>

