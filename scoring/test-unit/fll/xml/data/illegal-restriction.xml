<?xml version="1.0"?>
<fll
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  title="Illegal Restriction"
  xsi:noNamespaceSchemaLocation="http://fll-sw.sourceforge.net/fll.xsd"
  schemaVersion="1">

  <Performance
    minimumScore="0"
    weight="1.0">

    <goal
      initialValue="0"
      max="8"
      min="0"
      multiplier="5"
      name="crates_base"
      title="Crates in Base" />

    <goal
      initialValue="0"
      max="8"
      min="0"
      multiplier="2"
      name="crates"
      title="Crates Elsewhere on Table" />

    <restriction
      lowerBound="0"
      message="There are a maximum of 8 crates on the table"
      upperBound="8">
      <term>
        <constant value="1" />
        <goalRef
          goal="crates_base"
          scoreType="raw" />
      </term>
      <term>
        <constant value="1" />
        <goalRef
          goal="foo"
          scoreType="raw" />
      </term>
    </restriction>

    <tiebreaker>
      <test winner="HIGH">
        <term>
          <goalRef
            goal="crates_base"
            scoreType="computed" />
        </term>
      </test>
      <test winner="HIGH">
        <term>
          <goalRef
            goal="crates"
            scoreType="computed" />
        </term>
      </test>
    </tiebreaker>

  </Performance>

</fll>
