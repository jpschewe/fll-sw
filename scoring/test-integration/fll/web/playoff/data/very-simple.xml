<?xml version="1.0"?>
<fll
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  title="Simple Tournament"
  revision="1"
  xsi:noNamespaceSchemaLocation="http://fll-sw.sourceforge.net/fll.xsd"
  bracketSort="ALPHA_TEAM"
  schemaVersion="1">

  <Performance
    minimumScore="0"
    weight="1.0">

    <goal
      name="score"
      title="Score"
      max="100" />

    <tiebreaker>
      <!-- should be empty, but not allowed by the schema -->
      <test winner="HIGH">
        <term>
          <goalRef
            goal="score"
            scoreType="computed" />
        </term>
      </test>
    </tiebreaker>

  </Performance>

</fll>
