<?xml version="1.0"?>
<fll
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  title="initial value is below min, should fail"
  xsi:noNamespaceSchemaLocation="http://fll-sw.sourceforge.net/fll.xsd"
  schemaVersion="1">

  <Performance
    minimumScore="0"
    weight="1.0">

    <goal
      initialValue="0"
      max="5"
      min="2"
      multiplier="40"
      name="pump_station"
      title="Protect the Pump Station" />

    <goal
      initialValue="0"
      max="1"
      min="0"
      multiplier="30"
      name="flags"
      title="Transect Flags: Any Flags" />

    <goal
      initialValue="0"
      max="3"
      min="0"
      multiplier="15"
      name="flags_rows"
      title="Transect Flags: Number of Rows" />

    <!-- want to know the type of robot, but can't change the score, so set 
      the multiplier to 0 -->
    <goal
      name="robot_type"
      title="Robot Type"
      multiplier="0">
      <value
        score="1"
        title="RCX"
        value="rcx" />
      <value
        score="2"
        title="NXT"
        value="nxt" />
    </goal>

    <tiebreaker>
      <test winner="HIGH">
        <term>
          <goalRef
            goal="flags_rows"
            scoreType="computed" />
        </term>
      </test>
      <test winner="HIGH">
        <term>
          <goalRef
            goal="flags"
            scoreType="computed" />
        </term>
      </test>
      <test winner="HIGH">
        <term>
          <goalRef
            goal="pump_station"
            scoreType="computed" />
        </term>
      </test>
    </tiebreaker>

  </Performance>

</fll>
