<?xml version="1.0"?>
<fll
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  title="Minnesota Renewable Energy Challenge: 2014"
  xsi:noNamespaceSchemaLocation="http://fll-sw.sourceforge.net/fll.xsd"
  revision="1"
  schemaVersion="1">

  <Performance
    minimumScore="0"
    weight="0.35">

    <goal
      name="energy"
      title="Energy Output mWs"
      max="120000"
      scoreType="FLOAT" />

    <tiebreaker>
      <!-- should be empty, but not allowed by the schema -->
      <test winner="HIGH">
        <term>
          <goalRef
            goal="energy"
            scoreType="computed" />
        </term>
      </test>
    </tiebreaker>

  </Performance>

  <subjectiveCategory
    name="sail_car"
    title="Sail Car Challenge"
    weight="0.10">
