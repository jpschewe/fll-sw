<?xml version="1.0"?>
<fll
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  title="Science of Speed: 2009"
  bracketSort="ALPHA_TEAM"
  winner="LOW"
  revision="Rev: 351"
  xsi:noNamespaceSchemaLocation="http://fll-sw.sourceforge.net/fll.xsd"
  schemaVersion="1">

  <Performance
    minimumScore="-20"
    weight="1.0">

    <goal
