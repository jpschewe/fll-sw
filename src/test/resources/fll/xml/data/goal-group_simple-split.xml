<?xml version="1.0"?>
<fll xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    title="goal group split, should fail"
    xsi:noNamespaceSchemaLocation="http://fll-sw.sourceforge.net/fll.xsd"
    schemaVersion="1">

    <Performance minimumScore="0" weight="1.0">

        <goal category="one" initialValue="10" max="5" min="2"
            multiplier="40" name="pump_station"
            title="Protect the Pump Station" />

        <goal category="two" initialValue="0" max="1" min="0"
            multiplier="30" name="flags"
            title="Transect Flags: Any Flags" />

        <goal category="one" initialValue="0" max="3" min="0"
            multiplier="15" name="flags_rows"
            title="Transect Flags: Number of Rows" />


        <tiebreaker>
            <test winner="HIGH">
                <term>
                    <goalRef goal="pump_station"
                        scoreType="computed" />
                </term>
            </test>
        </tiebreaker>

    </Performance>

</fll>
