<?xml version="1.0"?>
<fll
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  title="2010 MN FIRST LEGO League Tournament"
  xsi:noNamespaceSchemaLocation="http://fll-sw.sourceforge.net/fll.xsd"
  revision="Rev: 5"
  schemaVersion="1">

  <Performance
    minimumScore="0"
    weight="1.0">

    <goal
      name="common_bone_repair"
      title="Common Arm Bone Repair"
      multiplier="25" />

    <goal
      name="bone_bridge_inserted"
      title="Leg Bone Bridge Inserted"
      multiplier="15" />

    <goal
      name="goal_scored"
      title="Goal Scored"
      multiplier="25" />

    <!-- if goal_scored, must have bone_bridge_inserted -->
    <restriction
      lowerBound="0"
      upperBound="1"
      message="You cannot have the Goal Scored without the Bone Bridge Inserted">
      <term>
        <constant value="-1" />
        <goalRef
          goal="goal_scored"
          scoreType="raw" />
      </term>
      <term>
        <constant value="1" />
        <goalRef
          goal="bone_bridge_inserted"
          scoreType="raw" />
      </term>
    </restriction>

    <goal
      name="rapid_blood_screening_syringe"
      title="Syringe Ever In Base"
      multiplier="25" />

    <goal
      name="rapid_blood_screening_cells"
      title="White and No Red Blood Cells in Patient Area"
      multiplier="15" />

    <goal
      name="red_blood_cells"
      title="Red Blood Cells"
      multiplier="5"
      max="8" />

    <goal
      name="bad_cell_destruction"
      title="Bad Cell Destruction"
      initialValue="0">
      <value
        score="25"
        title="Destruction"
        value="destruction" />
      <value
        score="20"
        title="Id"
        value="identification" />
      <value
        score="0"
        title="None"
        value="none" />
    </goal>

    <goal
      name="mechanical_arm_patent"
      title="Mechanical Arm Holding Patent"
      multiplier="25" />

    <goal
      name="cardiac_patch"
      title="Cardiac Patch"
      multiplier="20" />

    <goal
      name="pace_maker"
      title="Pace Maker"
      multiplier="25" />

    <goal
      name="nerve_mapping"
      title="Nerve Mapping"
      multiplier="15" />

    <goal
      name="object_control"
      title="Door Control Through Thought"
      multiplier="20" />

    <goal
      name="medicine_part1"
      title="Medicine Auto-dispensing: Blue and Whites off, pinks on"
      multiplier="25" />

    <goal
      name="medicine_part2"
      title="Blue/White/Container in Patient Area"
      multiplier="5" />

    <goal
      name="sensitivity"
      title="Robotic Sensitivity - Weight Up"
      multiplier="25" />

    <goal
      name="professional_teamwork"
      title="Professional Teamwork"
      multiplier="25" />

    <goal
      name="bionic_eyes"
      title="Bionic Eye Touching Upper Body"
      multiplier="20" />

    <goal
      name="stent"
      title="Stent Inserted"
      multiplier="25" />


    <tiebreaker>
      <test winner="HIGH">
        <term>
          <goalRef
            goal="goal_scored"
            scoreType="computed" />
        </term>
      </test>

      <test winner="HIGH">
        <term>
          <goalRef
            goal="pace_maker"
            scoreType="computed" />
        </term>
      </test>

      <test winner="HIGH">
        <term>
          <goalRef
            goal="professional_teamwork"
            scoreType="computed" />
        </term>
      </test>

      <test winner="HIGH">
        <term>
          <goalRef
            goal="bad_cell_destruction"
            scoreType="computed" />
        </term>
      </test>

      <test winner="HIGH">
        <term>
          <goalRef
            goal="mechanical_arm_patent"
            scoreType="computed" />
        </term>
      </test>

    </tiebreaker>

  </Performance>

