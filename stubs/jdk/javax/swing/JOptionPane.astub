package javax.swing;

import java.awt.Component;

import org.checkerframework.checker.nullness.qual.Nullable;
import org.checkerframework.checker.initialization.qual.UnknownInitialization;

public class JOptionPane extends JComponent implements Accessible {

  public JOptionPane(@Nullable @UnknownInitialization Object message, int messageType, int optionType,
                     @Nullable Icon icon);

  public JOptionPane(@Nullable @UnknownInitialization Object message, int messageType, int optionType,
                     @Nullable Icon icon, @Nullable Object[] options);

  public JOptionPane(@Nullable @UnknownInitialization Object message, int messageType, int optionType,
                     @Nullable Icon icon, @Nullable Object[] options, @Nullable Object initialValue);
  
  
  public static int showConfirmDialog(@Nullable @UnknownInitialization(Component.class) Component parentComponent,
                                      @Nullable @UnknownInitialization Object message) throws HeadlessException;
  public static int showConfirmDialog(@Nullable @UnknownInitialization(Component.class) Component parentComponent,
                                      @Nullable @UnknownInitialization Object message, String title, int optionType)
    throws HeadlessException;
  
  public static int showConfirmDialog(@Nullable @UnknownInitialization(Component.class) Component parentComponent,
                                      @Nullable @UnknownInitialization Object message, String title, int optionType, int messageType)
    throws HeadlessException;
  
  public static int showConfirmDialog(@Nullable @UnknownInitialization(Component.class) Component parentComponent,
                                      @Nullable @UnknownInitialization Object message, String title, int optionType,
