  public static String showInputDialog(@Nullable @UnknownInitialization(Component.class) Component parentComponent, @Nullable @UnknownInitialization Object message,
                                       Object initialSelectionValue);

  public static String showInputDialog(@Nullable @UnknownInitialization(Component.class) Component parentComponent,
                                       @Nullable @UnknownInitialization Object message, String title, int messageType)
    throws HeadlessException;

  public static Object showInputDialog(@Nullable @UnknownInitialization(Component.class) Component parentComponent,
                                       @Nullable @UnknownInitialization Object message, String title, int messageType, @Nullable Icon icon,
                                       Object[] selectionValues, Object initialSelectionValue)
    throws HeadlessException;

  public static int showInternalConfirmDialog(@Nullable @UnknownInitialization(Component.class) Component parentComponent,
                                              @Nullable @UnknownInitialization Object message);

  public static int showInternalConfirmDialog(@Nullable @UnknownInitialization(Component.class) Component parentComponent,
                                              @Nullable @UnknownInitialization Object message, String title,
                                              int optionType);

  public static int showInternalConfirmDialog(@Nullable @UnknownInitialization(Component.class) Component parentComponent,
                                              @Nullable @UnknownInitialization Object message,
                                              String title, int optionType,
                                              int messageType);

  public static int showInternalConfirmDialog(@Nullable @UnknownInitialization(Component.class) Component parentComponent,
                                              @Nullable @UnknownInitialization Object message,
                                              String title, int optionType,
                                              int messageType, @Nullable Icon icon);

  public static String showInternalInputDialog(@Nullable @UnknownInitialization(Component.class) Component parentComponent,
                                               @Nullable @UnknownInitialization Object message);

  public static String showInternalInputDialog(@Nullable @UnknownInitialization(Component.class) Component parentComponent,
                                               @Nullable @UnknownInitialization Object message, String title, int messageType) ;

  public static Object showInternalInputDialog(@Nullable @UnknownInitialization(Component.class) Component parentComponent,
                                               @Nullable @UnknownInitialization Object message, String title, int messageType, @Nullable Icon icon,
                                               Object[] selectionValues, Object initialSelectionValue);

  public static void showInternalMessageDialog(@Nullable @UnknownInitialization(Component.class) Component parentComponent,
                                               @Nullable @UnknownInitialization Object message);

  public static void showInternalMessageDialog(@Nullable @UnknownInitialization(Component.class) Component parentComponent,
                                               @Nullable @UnknownInitialization Object message, String title,
                                               int messageType);

  public static void showInternalMessageDialog(@Nullable @UnknownInitialization(Component.class) Component parentComponent,
                                               @Nullable @UnknownInitialization Object message,
                                               String title, int messageType,
                                               @Nullable Icon icon);

  public static int showInternalOptionDialog(@Nullable @UnknownInitialization(Component.class) Component parentComponent,
                                             @Nullable @UnknownInitialization Object message,
                                             String title, int optionType,
